{
  "main": "webpack.mix.js",
  "scripts": {
    "auth:gcp": "gcloud auth application-default login",
    "auth:gca": "npx google-artifactregistry-auth",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "watch": "npm run development -- --watch",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "eslint --ext .js,.vue,.ts src/",
    "lint:fix": "eslint --ext .js,.vue,.ts src/ --fix",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\"",
    "lint:style:fix": "stylelint \"src/**/*.{css,scss,vue}\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,scss,css}\"",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "prettier --write",
      "eslint --cache --fix",
      "bash -c 'if grep -l \"console\\.log\" $1; then echo \"‚ùå Error: console.log found in $1. Please remove before committing.\"; exit 1; fi' --"
    ],
    "*.{css,scss}": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --cache --fix",
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-modules-newlines": "^0.0.7",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.11",
    "laravel-mix": "^6.0.49",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.80.4",
    "sass-loader": "^16.0.2",
    "style-loader": "^3.3.3",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    "vue": "^3.4.21",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^2.0.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.13",
    "@floating-ui/vue": "^1.1.6",
    "@hostinger/hcomponents": "^3.0.3",
    "axios": "^1.6.7",
    "bootstrap-scss": "^5.3.3",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "ts-loader": "^9.5.1",
    "videojs-youtube": "^3.0.1",
    "vue-router": "^4.3.2",
    "vue3-toastify": "^0.2.1"
  }
}
